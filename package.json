    "@types/react-icons": "^2.2.7",
    "@types/sinon": "^10.0.13",
    "@types/slate-react": "0.22.9",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/tinycolor2": "^1.4.3",
